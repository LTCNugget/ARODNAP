{
  "manifest_version": 2,
  "name": "ARODNAP",
  "description": "This extension will take the album art and .m4a of a Pandora.com page, if it ever works",
  "version": "0.5",
  "browser_action": {
    "default_icon": "img/icon48.png"
  },
  "icons": {
    "48": "img/icon48.png"
  },
  "content_scripts": [ {
    "matches": ["pandora.com"],
    "js": ["js/content.js", "js/jQuery.js"]
  } ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}