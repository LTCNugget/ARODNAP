{
  "manifest_version": 2,
  "name": "ARODNAP",
  "description": "This extension will take the album art and .m4a of a Pandora.com page, if it ever works",
  "version": "0.5",
  "browser_action": {
    "default_icon": "img/icon48.png"
  },
  "icons": {
    "48": "icon48.png"
  },
  "content_scripts": [ {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  } ],
  "background": {
    "scripts": [
      "js/jQuery.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}