{
  "manifest_version": 2,
  "name": "ARODNAP",
  "description": "This extension will take the album art and .m4a of a Pandora.com page",
  "version": "4",
  "browser_action": {
  "default_icon": "img/icon.png"
  },
  "background": {
    "scripts": [
      "js/jQuery.js",
      "js/background.js",
      "js/content.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}